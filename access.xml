<!DOCTYPE access [
        <!ELEMENT access (enabled|disabled)*>
        <!ELEMENT enabled (#PCDATA)>
        <!ATTLIST enabled
                account CDATA #REQUIRED
                acl CDATA #REQUIRED>
        <!ELEMENT disabled (#PCDATA)>
        <!ATTLIST disabled
                acl CDATA #REQUIRED
                account CDATA #REQUIRED>
        ]>
<access>
    <enabled acl="view" account="MY_ROLE"/>
    <enabled acl="edit" account="MY_ATTRIBUTE"/>
    <enabled acl="pouet" account="Toto"/>
    <disabled acl="pouet" account="Toto"/>
</access>